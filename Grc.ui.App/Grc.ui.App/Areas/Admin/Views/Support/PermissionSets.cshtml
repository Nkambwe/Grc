@{
    Layout = "_AdminLayout";
    ViewData["Title"] = "PERMISSION SETS";
}
@model AdminDashboardModel
@{
    var component = new PageComponentViewModel
    {
        ComponentId = "1",
        ComponentKey = "PermissionSets",
        LeftActions = new List<ComponentAction>
        {
            new ComponentAction { ActionKey = ActionKey.NewSet.GetDescription(), Icon = "mdi mdi-plus", Tooltip = ILocalize.GetLocalizedLabel("App.Label.New") }
            // new ComponentAction { ActionKey = ActionKey.Separator.GetDescription()},
            // new ComponentAction { ActionKey = ActionKey.UserGroups.GetDescription(), Icon = "mdi mdi-account-group-outline", Tooltip = ILocalize.GetLocalizedLabel("App.Menu.Configurations.RoleGroups") }
        }
    };
}

<!-- permission set list -->
@await Component.InvokeAsync("BaseComponent", new { model = component, content = Component.InvokeAsync("PermissionSetList") })