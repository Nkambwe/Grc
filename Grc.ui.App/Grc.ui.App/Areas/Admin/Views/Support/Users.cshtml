@{
    Layout = "_AdminLayout";
    ViewData["Title"] = "SYSTEM USERS";
}
@model AdminDashboardModel
@{
    var component = new PageComponentViewModel
    {
        ComponentId = "1",
        ComponentKey = "Users",
        LeftActions = new List<ComponentAction> {
            new ComponentAction { ActionKey = ActionKey.AdminHome.GetDescription(), Icon = "mdi mdi-home-outline", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Home") },
            new ComponentAction { ActionKey = ActionKey.Separator.GetDescription()},
            new ComponentAction { ActionKey = ActionKey.NewUser.GetDescription(), Icon = "mdi mdi-account-multiple-plus-outline", Tooltip = ILocalize.GetLocalizedLabel("App.Label.New") },
            new ComponentAction { ActionKey = ActionKey.Separator.GetDescription()},
            new ComponentAction { ActionKey = ActionKey.ApproveUser.GetDescription(), Icon = "mdi mdi-account-multiple-check-outline", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Approve.User") },
            new ComponentAction { ActionKey = ActionKey.LockAccount.GetDescription(), Icon = "mdi mdi-account-lock-outline", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Lock.User") },
            new ComponentAction { ActionKey = ActionKey.Separator.GetDescription()},
            new ComponentAction { ActionKey = ActionKey.ExportToExcel.GetDescription(), Icon = "mdi mdi-file-excel-outline", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Export") }
        }
    };
}

<!-- user list -->
@await Component.InvokeAsync("BaseComponent", new { model = component, content = Component.InvokeAsync("UserList") })