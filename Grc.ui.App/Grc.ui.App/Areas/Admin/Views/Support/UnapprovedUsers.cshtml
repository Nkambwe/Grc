@{
    Layout = "_AdminLayout";
    ViewData["Title"] = "UNAPPROVED ACCOUNTS";
}
@model AdminDashboardModel
@{
    var component = new PageComponentViewModel
    {
        ComponentId = "1",
        ComponentKey = "UnapprovedUsers",
        LeftActions = new List<ComponentAction> {
            new ComponentAction { ActionKey = ActionKey.AdminHome.GetDescription(), Icon = "mdi mdi-home-outline", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Home") },
            new ComponentAction { ActionKey = ActionKey.Separator.GetDescription()},
            new ComponentAction { ActionKey = ActionKey.NewUser.GetDescription(), Icon = "mdi mdi-account-multiple-plus-outline", Tooltip = ILocalize.GetLocalizedLabel("App.Label.New") }
        }
    };
}

<!-- user list -->
@await Component.InvokeAsync("BaseComponent", new { model = component, content = Component.InvokeAsync("UnapprovedUserList") })