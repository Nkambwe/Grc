@{
    Layout = "_AdminLayout";
    ViewData["Title"] = "ROLE GROUP";
}
@model AdminDashboardModel
@{
    var component = new PageComponentViewModel
    {
        ComponentId = "1",
        ComponentKey = "RoleGroupPermissions",
        LeftActions = new List<ComponentAction>
        {
            new ComponentAction { ActionKey = ActionKey.AdminHome.GetDescription(), Icon = "mdi mdi-home-outlined", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Home") },
            new ComponentAction { ActionKey = ActionKey.RoleGroupNew.GetDescription(), Icon = "mdi mdi-plus", Tooltip = ILocalize.GetLocalizedLabel("App.Label.New") }
        }
    };
}

<!-- role list -->
@await Component.InvokeAsync("BaseComponent", new { model = component, content = Component.InvokeAsync("RoleGroupList") })