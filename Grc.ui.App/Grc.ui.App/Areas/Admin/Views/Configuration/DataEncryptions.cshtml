@{
    Layout = "_AdminLayout";
    ViewData["Title"] = "DATA ENCRYPTION";
}
@model AdminDashboardModel
@{
    var component = new PageComponentViewModel
    {
        ComponentId = "1",
        ComponentKey = "Encryption",
        LeftActions = new List<ComponentAction>
        {
            new ComponentAction { ActionKey = ActionKey.Search.GetDescription(), Icon = "mdi mdi-magnify", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Search") },
            new ComponentAction { ActionKey = ActionKey.Separator.GetDescription()},
            new ComponentAction { ActionKey = ActionKey.New.GetDescription(), Icon = "mdi mdi-plus", Tooltip = ILocalize.GetLocalizedLabel("App.Label.New") },
            new ComponentAction { ActionKey = ActionKey.Update.GetDescription(), Icon = "mdi mdi-plus", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Update") },
            new ComponentAction { ActionKey = ActionKey.Close.GetDescription(), Icon = "mdi mdi-plus", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Close") },
            new ComponentAction { ActionKey = ActionKey.Separator.GetDescription()},
            new ComponentAction { ActionKey = ActionKey.Email.GetDescription(), Icon = "mdi mdi-plus", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Email") },
            new ComponentAction { ActionKey = ActionKey.Excel.GetDescription(), Icon = "mdi mdi-pencil", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Export") }

        }
    };
}

<!-- role list -->
@await Component.InvokeAsync("BaseComponent", new { model = component, content = Component.InvokeAsync("EncryptionList") })