@model OperationsDashboardModel
@{
    Layout = "_OperationLayout";
    ViewData["Title"] = "PROCESS GROUPS";
}

@{
    var component = new PageComponentViewModel
    {
        ComponentId = $"0",
        ComponentKey = "ProcesseGroups",
        LeftActions = new List<ComponentAction>
        {
            new ComponentAction { ActionKey = ActionKey.OpsHome.GetDescription(), Icon = "mdi mdi-home-outline", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Home") },
            new ComponentAction { ActionKey = ActionKey.ProcessGroup.GetDescription(), Icon = "mdi mdi-plus", Tooltip = ILocalize.GetLocalizedLabel("App.Label.New") }
        }
    };
}

@await Component.InvokeAsync("BaseComponent", new { model = component, content = Component.InvokeAsync("ProcessGroupList") })