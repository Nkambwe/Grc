@{
    Layout = "_Layout";
    ViewData["Title"] = "REGULATORY OBLIGATIONS";
    var component = new PageComponentViewModel
    {
        ComponentId = $"0",
        ComponentKey = "RegulatoryMaps",
        LeftActions = new List<ComponentAction> {
            new ComponentAction {ActionKey = ActionKey.ComplianceHome.GetDescription(), Icon = "mdi mdi-home-outline", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Home")},
            new ComponentAction { ActionKey = ActionKey.Separator.GetDescription()},
            new ComponentAction { ActionKey = ActionKey.Excel.GetDescription(), Icon = "mdi mdi-microsoft-excel", Tooltip = "Summary Report" }
        }
    };
}

@await Component.InvokeAsync("BaseComponent", new { model = component, content = Component.InvokeAsync("ComplianceObligationRegister") })
