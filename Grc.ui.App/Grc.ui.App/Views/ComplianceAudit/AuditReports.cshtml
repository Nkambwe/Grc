@model UserDashboardModel
@{
    Layout = "_Layout";
    ViewData["Title"] = "AUDIT REPORTS";

}

@{
    var component = new PageComponentViewModel
    {
        ComponentId = $"0",
        ComponentKey = "Audit.Reports",
        LeftActions = new List<ComponentAction>
        {
            new ComponentAction { ActionKey = ActionKey.AuditHome.GetDescription(), Icon = "mdi mdi-home-outline", Tooltip = ILocalize.GetLocalizedLabel("App.Label.Home") },
            new ComponentAction { ActionKey = ActionKey.Separator.GetDescription()},
            new ComponentAction { ActionKey = ActionKey.AuditReport.GetDescription(), Icon = "mdi mdi-plus", Tooltip = ILocalize.GetLocalizedLabel("App.Label.New") }
        }
    };
}

@await Component.InvokeAsync("BaseComponent", new { model = component, content = Component.InvokeAsync("AuditReportList") })